package com.kh.java._import;

public class ImportMain {

	public static void main(String[] args) {
	
		// 1. 객체생성: 클래스->객체
		// 객체 recipe
		// 클래스명 변수명 = new 생성자;
		ImportMain main = new ImportMain();
		
		// 2. 메소드 호출
		main.test1();
		
		
		// 1. 객체생성
		Foo foo = new Foo();
		// 2. 메소드 호출
		foo.test2();
	}
	
	public void test1() {
		System.out.println("안녕");
	}
}
