package com.kh.java.value;

public class ValueMain {
	
	public static void main(String[] args) {
		System.out.println("시작");
		
		// 1. 객체(메모리조각) 생성
		// 객체레시피: 클래스>객체
		// 클래스명 변수명 = new 클래스명();
		ValueMain main = new ValueMain();
		
		// 2. 객체의 메소드 호출
		// 변수명.메소드명;
		main.test1();
		
		System.out.println("끝");
	}
	
	public void test1() {
		System.out.println("테스트");
	}

}
